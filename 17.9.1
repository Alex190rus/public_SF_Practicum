def binary_search(array, key):
    low_bount = 0
    upp_bount = len(array) - 1

    while low_bount <= upp_bount:
        center = (low_bount + upp_bount) // 2

        if array[center] == key:
            return center

        elif array[center] > key:
            upp_bount = center - 1

        elif array[center] < key:
            low_bount = center + 1

    return None

